	"version": "0.2.0",
	"configurations": [
		{
			"name": "Practice .NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build-practice",
			"program": "${workspaceFolder}/practice-round/src/bin/Debug/netcoreapp3.1/google-hashcode-2020.dll",
			"args": [],
			"cwd": "${workspaceFolder}/practice-round/src/",
			"console": "internalConsole",
			"stopAtEntry": false
		},
		{
			"name": "Qualification .NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build-qualification",
			"program": "${workspaceFolder}/qualification-round/src/bin/Debug/netcoreapp3.1/google-hashcode-2020.dll",
			"args": [],
			"cwd": "${workspaceFolder}/qualification-round/src/",
			"console": "internalConsole",
			"stopAtEntry": false
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}
